// violence

int maxArea(int* height, int heightSize) {
    int i, j;
    int maxarea = 0;
    for (i = 0; i < heightSize; ++i)
    {
        for (j = i + 1; j < heightSize; ++j)
        {
            maxarea = max(maxarea, (j-i) * (min(height[i], height[j]) ));
        }
    }
    
    return maxarea;
}

int max(int i, int j)
{
    return (i > j) ? i : j;
}

int min(int i, int j)
{
    return (i < j) ? i : j;
}
